<Window x:Class="SpotifyImporter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            
        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button x:Name="Do_Things" Content="Do Things"  Grid.Column="0" Grid.Row ="1" HorizontalAlignment="Center" VerticalAlignment="Center" Width="75" Click="Do_Things_Click"/>
        
    </Grid>
</Window>
